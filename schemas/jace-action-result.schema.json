{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://jace-spec.github.io/schemas/jace-action-result.schema.json",
  "type": "object",
  "properties": {
    "ok": {
      "type": "boolean"
    },
    "dry_run": {
      "type": "boolean"
    },
    "state": {},
    "result": {},
    "audit_preview": {
      "type": "string"
    },
    "events": {
      "type": "array",
      "items": {
        "$ref": "https://jace-spec.github.io/schemas/jace-event.schema.json"
      }
    },
    "error": {
      "type": "object",
      "required": [
        "code"
      ],
      "properties": {
        "code": {
          "enum": [
            "INVALID_PARAMS",
            "UNAUTHORIZED",
            "FORBIDDEN",
            "NOT_FOUND",
            "CONFLICT",
            "PRECONDITION_FAILED",
            "RATE_LIMITED",
            "POLICY_DENIED",
            "INTERNAL",
            "DRY_RUN_UNSUPPORTED"
          ]
        },
        "message": {
          "type": "string"
        },
        "hints": {
          "type": "array",
          "items": {
            "type": "string"
          }
        },
        "details": {
          "type": "object",
          "additionalProperties": true
        }
      },
      "additionalProperties": false
    }
  },
  "additionalProperties": false
}
